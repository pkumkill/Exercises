class Node {
    int item;
    int somethingElse;

    public Node(int item,int somethingElse){
        this.item = item;
        this.somethingElse = somethingElse;
    }
}

LinkedList l = new LinkedList(new Node(1,2));

l.add(new Node(1,2),2);